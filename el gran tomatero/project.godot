; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[android]

Modules_old="org/godotengine/godot/GodotAdMob"

[application]

config/name="el gran tomatero"
run/main_scene="res://scenes/menu.tscn"
boot_splash/image="res://img/loading.png"
boot_splash/bg_color=Color( 0.601563, 0.15274, 0.15274, 1 )
config/icon="res://img/player/Tomatero_idle_right3.png"

[autoload]

maestro="*res://scripts/maestro.gd"

[display]

window/size/width=360
window/size/height=540
window/size/resizable=false
window/energy_saving/keep_screen_on=false
window/handheld/orientation="portrait"
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[gui]

theme/custom_font="res://theme/retro_font1.tres"

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="C:\\Users\\andre\\Documents\\el gran tomatero\\debug"
file_logging/max_log_files=2

[rendering]

environment/default_environment="res://default_env.tres"
